{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "cosmosDB",
      "name": "rateitems",
      "databaseName": "%CosmosDBName%",
      "collectionName": "ratings",
      "createIfNotExists": "false",
      "connectionStringSetting": "AzureCosmosDBConnectionString",
      "direction": "in",
      "Id": "{Query.id}",
      "PartitionKey": "{Query.id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
